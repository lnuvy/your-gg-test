{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@pages": "./src/../pages",
          "@hooks": "./src/hooks",
          "@components": "./src/components",
          "@elements": "./src/elements",
          "@typing": "./src/typing",
          "@queries": "./src/queries",
          "@constant": "./src/constant"
        }
      }
    ],
    [
      "@emotion/babel-plugin"
    ]
  ]
}